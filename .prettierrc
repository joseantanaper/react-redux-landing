{
  "prettier/prettier": [
    "error",
    {
      "endOfLine": "auto"
    }
  ],
  "prettier": "prettier-config-nick",
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "endOfLine": "crlf",
  "overrides": [
    {
      "files": ["*.json"],
      "options": {
        "parser": "json"
      }
    }
  ]
}
